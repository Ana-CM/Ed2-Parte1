{
 "tasks": [
  {
   "type": "shell",
   "label": "g++ build active sfmlfile",
   "command": "C:\\MinGW\\bin\\g++.exe",
   "args": [
    "-g",
    "${file}",
    "-o",
    "${fileDirname}\\${fileBasenameNoExtension}.exe",
    "${fileDirname}/Datasetf/Datasetf.cpp",
    "${fileDirname}/Dataset/Dataset.cpp"
   ],
   "options": {
    "cwd": "${workspaceFolder}"
   },
   "problemMatcher": [
    "$gcc"
   ],
   "group": {
    "kind": "build",
    "isDefault": true
   }
  }
 ],
 "version": "2.0.0"
}